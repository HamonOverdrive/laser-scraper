{
    "name": "react-electron-web-boilerplate",
    "productName": "React Electron & Web Boilerplate",
    "version": "0.1.0",
    "description": "React Electron & Web Boilerplate",
    "author": "Mikey Fried Chicken",
    "homepage": "http://github",
    "license": "MIT",
    "main": "main.js",
    "private": true,
    "eslint": { "ignorePath": ".eslintignore" },
    "scripts": {
        "start": "webpack-dev-server --open --config webpack.dev.js --progress --color",
        "build": "webpack --config webpack.prod.js",
        "build-dev": "webpack --config webpack.dev.js --progress --color",
        "lint": "eslint -c .eslintrc.json src/** src/**/**; exit 0",
        "flow": "flow",
        "flow start": "flow start",
        "flow stop": "flow stop",
        "flow status": "flow status",
        "flow coverage": "flow coverage",
        "electron-prod": "webpack --config webpack.prod.electron.js && electron --noDevServer .",
        "electron-dev": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.electron.js",
        "electron-build": "webpack --config webpack.prod.electron.js",
        "electron-package": "electron-packager ./electron/prod react-electron-web-boilerplate --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/favicon.ico --prune=true --out=electron/package --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"React Electron & Web Boilerplate\""
    },
